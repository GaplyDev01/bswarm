{
  "name": "txbt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run remove-console-logs && next build",
    "build:deploy": "npm run fix-types && npm run add-ts-ignore && npm run remove-console-logs && cross-env TS_NODE_PROJECT=tsconfig.prod.json next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "typecheck:deploy": "tsc --noEmit --project tsconfig.prod.json",
    "fix-types": "node scripts/fix-unknown-types.js",
    "add-ts-ignore": "node scripts/add-ts-ignore.js",
    "remove-console-logs": "node scripts/remove-console-logs.js",
    "check-console-logs": "node scripts/remove-console-logs.js --check",
    "check-security": "npm audit --production",
    "preflight": "npm run lint && npm run typecheck && npm run check-console-logs && npm run check-security",
    "deploy": "npm run build:deploy && npm run verify-deployment",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "verify-deployment": "node scripts/verify-deployment.js"
  },
